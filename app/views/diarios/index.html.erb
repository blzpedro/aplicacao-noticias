<p id="notice"><%= notice %></p>

<h1>Diarios</h1>

<table>
  <thead>
    <tr>
      <th>Titulo</th>
      <th>Corpo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @diarios.each do |diario| %>
      <tr>
        <td><%= diario.titulo %></td>
        <td><%= diario.corpo %></td>
        <td><%= link_to 'Show', diario %></td>
        <td><%= link_to 'Edit', edit_diario_path(diario) %></td>
        <td><%= link_to 'Destroy', diario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Diario', new_diario_path %>
